{
  "name": "@gnosis.pm/singleton-deployer-example",
  "version": "1.0.0",
  "description": "Example how to use the singleton factory (EIP 2470) to deploy a contract",
  "homepage": "https://github.com/gnosis/singleton-deployer/example",
  "license": "GPL-3.0",
  "files": [
    "test"
  ],
  "scripts": {
    "test": "run(){ run-with-testrpc -l 20000000 --noVMErrorsOnRPCResponse \"truffle test $@\"; }; run",
    "compile": "run(){ truffle compile $@; }; run",
    "coverage": "run(){ truffle run coverage $@; }; run",
    "deploy": "run(){ truffle deploy --network=$@; }; run",
    "do": "run(){ truffle exec --network=$@; }; run",
    "build": "echo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnosis/singleton-deployer.git"
  },
  "keywords": [
    "Ethereum"
  ],
  "author": "stefan@gnosis.pm",
  "bugs": {
    "url": "https://github.com/gnosis/singleton-deployer/issues"
  },
  "devDependencies": {
    "@gnosis.pm/singleton-deployer-eip2470-factory": "^1.0.0",
    "@gnosis.pm/singleton-deployer-yul-factory": "^1.0.0",
    "@gnosis.pm/singleton-deployer-web3js-provider": "^1.0.0",
    "@gnosis.pm/singleton-deployer-truffle": "^1.0.0",
    "@truffle/hdwallet-provider": "^1.0.0",
    "dotenv": "^8.0.0",
    "ganache-cli": "6.3.0",
    "ipfs-http-client": "^44.2.0",
    "random-buffer": "*",
    "run-with-testrpc": "^0.3.0",
    "truffle": "^5.1.42",
    "web3": "^1.2.6"
  },
  "dependencies": {
    "solc": "0.7.2"
  },
  "resolutions": {
    "bitcore-lib": "8.1.1",
    "run-with-testrpc/ganache-cli": "6.3.0"
  }
}